      "code" : "
operator core_array_rescale_task<<<index>>>($TYPE$ value, $TYPE$ inMin, $TYPE$ inMax, $TYPE$ outMin, $TYPE$ outMax, io $TYPE$ result) {
  result[index] = outMin[index < outMin.size() ? index : outMin.size() - 1] + 
    (
      outMax[index < outMax.size() ? index : outMax.size() - 1] - 
      outMin[index < outMin.size() ? index : outMin.size() - 1]
    ) * (
      value[index < value.size() ? index : value.size() - 1] - 
      inMin[index < inMin.size() ? index : inMin.size() - 1]
    ) / (
      inMax[index < inMax.size() ? index : inMax.size() - 1] - 
      inMin[index < inMin.size() ? index : inMin.size() - 1]
    );
}

dfgEntry {
  if(value.size() == 0) return;
  if(inMin.size() == 0) return;
  if(inMax.size() == 0) return;
  if(outMin.size() == 0) return;
  if(outMax.size() == 0) return;
  Size size = value.size() > inMin.size() ? value.size() : inMin.size();
  size = inMax.size() > size ? inMax.size() : size;
  size = outMin.size() > size ? outMin.size() : size;
  size = outMax.size() > size ? outMax.size() : size;
  result.resize(size);

  if(size < 10000)
  {
    for(Size i=0;i<size;i++)
      core_array_rescale_task(i, value, inMin, inMax, outMin, outMax, result);
  }
  else
  {
    core_array_rescale_task<<<size>>>(value, inMin, inMax, outMin, outMax, result);
  }
}
