      "presetGUID" : "DB7696377935DC51D52B090F9EEC5BA8",
      "code" : "dfgEntry {
  if(hair == null)
    return;

  if(mode == 0 || mode == 2) // per guide or both
  {    
    for(Size i=0;i<hair.guides.size();i++)
    {
      if(!hair.guides[i].isValid())
        continue;
      hair.guides[i].setPoints(dfgPullBlockPort('deformPerGuide.result', hair.guides[i], i, hair.guides.size()));
    }
  }
  if(mode == 1 || mode == 2) // per vertex or both
  {    
    for(Size i=0;i<hair.guides.size();i++)
    {
      if(!hair.guides[i].isValid())
        continue;
      for(Size j=0;j<hair.guides[i].points.size();j++)
      {
        Float32 ratio = Float32(j) / Float32(hair.guides[i].points.size() - 1);
        hair.guides[i].points[j] = dfgPullBlockPort('deformPerVertex.result', 
          hair.guides[i], 
          hair.guides[i].points[j],
          hair.guides[i].normal,
          hair.guides[i].color,
          j,
          ratio,
          hair.guides[i].points.size(),
          i, hair.guides.size());
      }
    }
  }
}
"
