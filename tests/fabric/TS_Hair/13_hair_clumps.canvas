      "code" : "dfgEntry {
  if(hair == null)
    return;
    
  Integer idxs[] = indices.clone();
  if(idxs.size() == 0)
  {
    for(Size i=0;i<hair.guides.size();i++)
    {
      idxs.push(i);
    }
  }

  for(Size i=0;i<idxs.size();i++)
  {
    Integer idx = idxs[i];
    if(!hair.guides[idx].isValid())
      continue;
    hair.guides[idx] = dfgPullBlockPort('edit.result', hair.guides[idx], idx, hair.guides.size());
  }
}
